{
  "name": "ssu-it-locker",
  "version": "0.0.1",
  "description": "SSU IT Collage Locker system.",
  "private": true,
  "scripts": {
    "build": "pnpm build -r",
    "dev": "pnpm create-network --if-present && pnpm start-db -r && pnpm dev -r",
    "create-network": "docker network create local-dev",
    "delete": "pnpm delete --filter=*-frontend && pnpm delete --filter=*-lambda",
    "deploy": "pnpm deploy --filter=*-lambda && pnpm deploy --filter=*-frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EATSTEAK/ssu-it-locker.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "author": "EATSTEAK <me@eatsteak.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EATSTEAK/ssu-it-locker/issues"
  },
  "homepage": "https://github.com/EATSTEAK/ssu-it-locker#readme"
}
