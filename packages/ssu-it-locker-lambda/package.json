{
  "name": "ssu-it-locker-lambda",
  "version": "0.0.1",
  "private": true,
  "description": "Backend Lambda script for ssu-it-locker",
  "main": "src/index.js",
  "author": "EATSTEAK <me@eatsteak.dev>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "scripts": {
    "build": "sam build",
    "dev": "sam build && cross-env AWS_SAM_LOCAL=true sam local start-api --docker-network local-dev",
    "start-db": "docker-compose down && docker-compose up -d && aws dynamodb create-table --region ap-southeast-2 --endpoint http://localhost:8000 --cli-input-json file://db-table-cli.json",
    "stop-db": "docker-compose down",
    "deploy": "sam deploy --profile globalmedia --no-confirm-changeset",
    "delete": "sam delete --no-prompts",
    "test": "mocha tests/unit",
    "integ-test": "mocha tests/integration"
  },
  "devDependencies": {
    "aws-sdk": "^2.1048.0",
    "chai": "^4.2.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "mocha": "^8.2.1"
  }
}
